{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "property-no-unknown": [
            true,
            {
                "ignoreProperties": [
                    "themeDark"
                ]
            }
        ],
    "selector-pseudo-class-no-unknown": [
            true,
            {
                "ignorePseudoClasses": [
                    "export",
                    "global"
                ]
            }
        ],
  }
}
